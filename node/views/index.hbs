<!DOCTYPE HTML>
<html>
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
		<title>{{title}}</title>
		<style type="text/css">
body{margin: 0;}
		</style>
	</head>
	<body>
		<div>Index Of {{path}}</div>
		{{#if menu}}
		<div><a href="javascript: location.reload();">refresh</a></div>
		{{/if}}
		{{#unless menu}}
		<div><a href="javascript: void(0);">menu</a></div>
		{{/unless}}
		<table>
			<tr><th>filename</th><th>size</th><th>ctime</th></tr>
			<tr><td><a href="../">../</a></td><td>0</td><td>-</td></tr>
			{{#each files}}
			<tr><td><a href="{{this.href}}">{{this.fn}}</a></td><td>{{this.size}}</td><td>{{this.ctime}}</td></tr>
			{{else}}
			<!-- no data -->
			{{/each}}
		</table>
		{{! <!-- --> }}
		{{#with info}}
		<!-- {{message}} -->
		{{/with}}
	</body>
</html>

